MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin123456
# Console settings
MINIO_BROWSER_REDIRECT_URL=http://localhost:9001


# ============================================================
# NETWORK THROTTLING PRESETS
# ============================================================
# Uncomment một trong các preset bên dưới để test
# hoặc tự customize các giá trị
# ============================================================

# ------------------- PRESET 1: DISABLED (Full Speed) -------------------
# Không giới hạn, chạy tốc độ tối đa
#THROTTLE_ENABLED=false

# ------------------- PRESET 2: FAST 4G/WiFi -------------------
# Mô phỏng mạng 4G LTE hoặc WiFi tốt
#THROTTLE_ENABLED=true
#BANDWIDTH_LIMIT=50mbit
#LATENCY_MS=20
#LATENCY_JITTER_MS=5
#PACKET_LOSS_PERCENT=0

# ------------------- PRESET 3: MODERATE 4G -------------------
# Mô phỏng mạng 4G trung bình (DEFAULT)
THROTTLE_ENABLED=true
BANDWIDTH_LIMIT=10mbit
LATENCY_MS=50
LATENCY_JITTER_MS=10
PACKET_LOSS_PERCENT=0

# ------------------- PRESET 4: SLOW 3G -------------------
# Mô phỏng mạng 3G chậm
#THROTTLE_ENABLED=true
#BANDWIDTH_LIMIT=2mbit
#LATENCY_MS=150
#LATENCY_JITTER_MS=50
#PACKET_LOSS_PERCENT=1

# ------------------- PRESET 5: POOR CONNECTION -------------------
# Mô phỏng mạng rất yếu (edge cases testing)
#THROTTLE_ENABLED=true
#BANDWIDTH_LIMIT=512kbit
#LATENCY_MS=300
#LATENCY_JITTER_MS=100
#PACKET_LOSS_PERCENT=3

# ------------------- PRESET 6: UNSTABLE NETWORK -------------------
# Mô phỏng mạng không ổn định (high jitter + packet loss)
#THROTTLE_ENABLED=true
#BANDWIDTH_LIMIT=5mbit
#LATENCY_MS=100
#LATENCY_JITTER_MS=80
#PACKET_LOSS_PERCENT=5